<main id="main">
	<div class="title">
		<div class="titles">
			<h2>Instagram</h2>
			<p>{{Page.pagename}} - Ticket List</p>
		</div>
	</div>
		{{Ticket}}
	<div class="content">
		<div class="row">
			<h3 class="col-sm-3">Comment</h3>
			<h3 class="col-sm-3">From</h3>
			<h3 class="col-sm-3">Status</h3>
			<h3 class="col-sm-3">Details</h3>
		</div>
		{{#each Ticket}}
		<div class="row">
			<p class="col-sm-3">{{comment}}</p>
			<p class="col-sm-3">{{fromName}}</p>
			<p class="col-sm-3">{{status}}</p>
			<p class="col-sm-3"><a href="/instagram/ticket/detail?Id={{../Id}}&Commentid={{comId}}">View Details</a></p>
		</div>
		{{/each}}
	</div>
</main>
